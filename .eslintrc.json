{
  "env": {
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "consistent-return": "off",
    "no-param-reassign": "off"
  }
}
